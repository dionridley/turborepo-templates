{
  "name": "vite-react-storybook-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "test": "turbo test",
    "storybook": "turbo storybook",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "turbo": "^2.5.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7"
  },
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=20.19.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  }
}